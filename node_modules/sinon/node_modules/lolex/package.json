    "referee": "1.2.0",
    "sinon": "4.5.0"
  },
  "eslintConfig": {
    "extends": "eslint-config-sinon",
    "plugins": [
      "ie11"
    ],
    "rules": {
      "ie11/no-collection-args": [
        "error"
      ],
      "ie11/no-for-in-const": [
        "error"
      ],
      "ie11/no-loop-func": [
        "warn"
      ],
      "ie11/no-weak-collections": [
        "error"
      ]
    }
  },
